<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DEXTESTING TESTEVENT1 PRD" parallel="tests" thread-count="3">
    <parameter name="USE_CASE" value="dextesting-testevent1" />
    <test name="Metadata Verify">
        <parameter name="SENDER_MANIFEST" value="dextesting-testevent1.properties"/>
        <classes>
            <class name="MetadataVerify" />
        </classes>
    </test>
    <test name="File Copy">
        <parameter name="SENDER_MANIFEST" value="dextesting-testevent1.properties"/>
        <classes>
            <class name="FileCopy" />
        </classes>
    </test>
    <test name="Proc Stat">
        <parameter name="SENDER_MANIFEST" value="dextesting-testevent1.properties"/>
        <!-- Define parameters for expected URLs as comma-separated lists -->
        <parameter name="EXPECTED_SOURCE_URL_PREFIXES" value="https://ocioededataexchangeprd.blob.core.windows.net/dextesting-testevent1"/>
        <parameter name="EXPECTED_DESTINATION_URL_PREFIXES" value="https://ocioederoutingdatasaprd.blob.core.windows.net/routeingress/dextesting-testevent1,https://edavprddatalakedexprd.blob.core.windows.net/upload/dextesting-testevent1"/>
        <groups>
            <run>
                <exclude name="proc-stat-trace" />
            </run>
        </groups>
        <classes>
            <class name="ProcStat" />
        </classes>
    </test>
</suite>