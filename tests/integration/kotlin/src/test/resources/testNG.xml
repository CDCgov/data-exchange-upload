<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DEX Upload API Suite" parallel="methods" thread-count="1">
    <test name="Generic Test Cases">
        <parameter name="SENDER_MANIFEST" value="ndlp-rsvprevention.properties"/>
        <!-- Define parameters for expected URLs as comma-separated lists -->
        <parameter name="EXPECTED_SOURCE_URL_PREFIXES" value="https://ocioededataexchangedev.blob.core.windows.net/ndlp-rsvprevention"/>
        <parameter name="EXPECTED_DESTINATION_URL_PREFIXES" value="https://ocioederoutingdatasadev.blob.core.windows.net/routeingress/ndlp-rsvprevention"/>
        <classes>
            <class name="ProcStat">
                <methods>
                    <include name="shouldHaveMetadataVerifyReportWhenFileUploaded"/>
                    <include name="shouldHaveNullIssuesArrayWhenFileUploaded"/>
                    <include name="shouldHaveUploadStatusReportWhenFileUploaded"/>
                    <include name="shouldHaveEqualOffsetAndSizeWhenFileUploaded"/>
                    <include name="shouldHaveValidDestinationAndSourceURLWhenFileUploaded"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>



