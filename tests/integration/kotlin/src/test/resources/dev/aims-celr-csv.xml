<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AIMS CELR CSV DEV" parallel="tests" thread-count="3">
    <parameter name="USE_CASE" value="aims-celr-csv" />
    <test name="File Copy">
        <parameter name="SENDER_MANIFEST" value="aims-celr-csv.properties"/>
        <classes>
            <class name="FileCopy">
                <methods>
                    <exclude name="shouldCopyToEdavContainer" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Metadata Verify">
        <parameter name="SENDER_MANIFEST" value="aims-celr-csv.properties"/>
        <classes>
            <class name="MetadataVerify" />
        </classes>
    </test>
    <test name="Proc Stat">
        <parameter name="SENDER_MANIFEST" value="aims-celr-csv.properties"/>
        <!-- Define parameters for expected URLs as comma-separated lists -->
        <parameter name="EXPECTED_SOURCE_URL_PREFIXES" value="https://ocioededataexchangedev.blob.core.windows.net/aims-celr-csv"/>
        <parameter name="EXPECTED_DESTINATION_URL_PREFIXES" value="https://ocioederoutingdatasadev.blob.core.windows.net/routeingress/aims-celr-csv"/>
        <groups>
            <run>
                <exclude name="proc-stat-trace" />
            </run>
        </groups>
        <classes>
            <class name="ProcStat" />
        </classes>
    </test>
</suite>