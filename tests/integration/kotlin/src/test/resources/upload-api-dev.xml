<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DEX Upload API Suite DEV" parallel="tests" thread-count="1">
    <test name="PULSENET-LOCALSEQUENCEFILE">
        <parameter name="SENDER_MANIFEST" value="pulsenet-localsequencefile.properties"/>
        <!-- Define parameters for expected URLs as comma-separated lists -->
        <parameter name="EXPECTED_SOURCE_URL_PREFIXES" value="https://ocioededataexchangedev.blob.core.windows.net/pulsenet-localsequencefile"/>
        <parameter name="EXPECTED_DESTINATION_URL_PREFIXES" value="https://edavdevdatalakedex.blob.core.windows.net/upload/ndlp-localsequencefile"/>
        <groups>
            <run>
                <include name="file-copy" />
                <include name="proc-stat" />
                <include name="metadata-verify" />
                <exclude name="proc-stat-trace" />
            </run>
        </groups>
        <classes>
            <class name="ProcStat" />
            <class name="MetadataVerify" />
            <class name="FileCopy">
                <methods>
                    <exclude name="shouldCopyToRoutingContainer" />
                </methods>
            </class>
        </classes>
    </test>
