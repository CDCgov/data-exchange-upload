<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AIMS CELR HL7 DEV" parallel="classes" thread-count="1">
    <parameter name="USE_CASE" value="aims-celr-hl7" />
    <test name="aims-celr-hl7">
        <parameter name="SENDER_MANIFEST" value="aims-celr-hl7.properties"/>
        <!-- Define parameters for expected URLs as comma-separated lists -->
        <parameter name="EXPECTED_SOURCE_URL_PREFIXES" value="https://ocioededataexchangedev.blob.core.windows.net/aims-celr-hl7"/>
        <parameter name="EXPECTED_DESTINATION_URL_PREFIXES" value="https://ocioederoutingdatasadev.blob.core.windows.net/routeingress/aims-celr-hl7"/>
        <groups>
            <run>
                <include name="file-copy" />
                <include name="proc-stat" />
                <include name="metadata-verify" />
                <exclude name="proc-stat-trace" />
            </run>
        </groups>
        <classes>
            <class name="ProcStat" />
            <class name="MetadataVerify" />
            <class name="FileCopy">
                <methods>
                    <exclude name="shouldCopyToEdavContainer" />
                </methods>
            </class>
        </classes>
    </test>
</suite>